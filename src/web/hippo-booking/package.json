    "@vitejs/plugin-react": "^4.5.2",
    "canvas": "^3.1.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "prettier": "3.5.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^3.2.3"
