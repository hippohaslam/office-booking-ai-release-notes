{
  "name": "hippo-booking",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "yarn@4.2.2",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "e2e": "vite --mode e2e",
