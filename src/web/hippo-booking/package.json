    "fabric": "^5.4.2",
    "jwt-decode": "^4.0.0",
    "pa11y-ci": "^3.1.0",
    "react": "^18.3.1",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.28.0",
    "uuid": "^11.0.3"
