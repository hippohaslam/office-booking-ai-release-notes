    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.3.4",
    "canvas": "^2.11.2",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "^25.0.1",
    "msw": "^2.7.3",
    "prettier": "3.5.3",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "vite": "5.4.15",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^2.1.9"
